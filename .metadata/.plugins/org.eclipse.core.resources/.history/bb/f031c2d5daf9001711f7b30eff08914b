	private SqlSessionFactory sqlSessionFactory;
	
	public SqlSessionFactory getSQLSessionFactory() {
		
		
		String resource = "min/edu/mybatis/Configuration.xml";
		
	
		Reader reader = Resources.getResourceAsReader(resource);
	}
