SELECT SEQ,USER_ID,USER_PW,USER_NAME,EMAIL,NVL(USER_RECORD,0) AS user_record ,REGDATE,GRADE,ENABLED FROM ROOMUSER WHERE USER_ID='15queue' AND USER_PW='10041004' AND ENABLED='Y'



INSERT INTO HK.ROOMUSER
